{
  "name": "reearth-visualizer-plugin-shadcn-template",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "build": "run-s build:visualizer zip",
    "preview": "vite preview --port 5005 --strict-port",
    "zip": "node ./scripts/zip.mjs",
    "manage": "node ./scripts/manage.mjs",
    "type": "tsc -b",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "format": "prettier --write .",
    "dev-build": "concurrently 'yarn dev:inspector_block:main' 'yarn build:inspector_block:main --watch' 'yarn build:inspector_block:extension --watch' 'yarn build:visualizer:main --watch' 'yarn build:visualizer:extension --watch' 'vite preview --port 5005 --strict-port'",
    "build:visualizer:extension": "cross-env EXTENSION_NAME=visualizer vite build -c configs/extension.ts",
    "build:visualizer": "run-s build:visualizer:main build:visualizer:extension",
    "dev:inspector_block:main": "cross-env EXTENSION_NAME=inspector_block UI_NAME=main vite -c configs/ui.ts",
    "build:inspector_block:main": "cross-env EXTENSION_NAME=inspector_block UI_NAME=main vite build -c configs/ui.ts",
    "build:inspector_block:extension": "cross-env EXTENSION_NAME=inspector_block vite build -c configs/extension.ts",
    "build:inspector_block": "run-s build:inspector_block:main build:inspector_block:extension",
    "dev:visualizer:main": "cross-env EXTENSION_NAME=visualizer UI_NAME=main vite -c configs/ui.ts",
    "build:visualizer:main": "cross-env EXTENSION_NAME=visualizer UI_NAME=main vite build -c configs/ui.ts"
  },
  "dependencies": {
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-slot": "1.2.3",
    "@tailwindcss/vite": "4.1.10",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.516.0",
    "tailwind-merge": "2.5.5",
    "tailwindcss-animate": "1.0.7",
    "tw-animate-css": "1.3.4"
  },
  "devDependencies": {
    "@reearth/core": "0.0.7-alpha.38",
    "@tailwindcss/postcss": "4.1.10",
    "@types/node": "20.14.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "@vitejs/plugin-react": "4.5.2",
    "archiver": "7.0.1",
    "autoprefixer": "10.4.21",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "9.17.0",
    "eslint-config-reearth": "0.3.8",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "fast-glob": "3.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "prettier": "3.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "4.1.10",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vite-plugin-cdn-import": "1.0.1",
    "vite-plugin-external": "6.2.2",
    "vite-plugin-singlefile": "2.2.0",
    "yml": "1.0.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "packageManager": "yarn@4.5.1"
}
